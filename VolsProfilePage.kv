#:kivy 1.0.8 
   

<FFTmain>:
	canvas:
		Color:
			rgba: 255,255,255,255
		Rectangle:
			size: root.width, root.height

	BoxLayout:
		id: findvol_layout
		pos: 40, 20
		size: root.width - 80, root.height - 40
		spaceing:0.1
		orientation:'vertical'
		BoxLayout:
			orientation: 'horizontal'
			Image:
				source: "images/noy.jpg"
				size_hint: .5, .9
			Label:
				id: vol_rating
				color: 0, 0, 0, 1
				size_hint: .5, 0.1
				text: "Noy Galron"
		BoxLayout:
			orientation: 'horizontal'
			Label:
				id: vol_rating
				color: 0, 0, 0, 1
				size_hint: .5, 0.1
				text: "noygalron@gmail.com"
			Button:
				size_hint: 0.3, 0.1
				text: "PICK ME UP!!!"
		BoxLayout:
			orientation: 'horizontal'
			Label:
				id: vol_rating
				color: 0, 0, 0, 1
				size_hint: 0.4, 0.1
				text: "Gender: Female"
			Button:
				size_hint: 0.5, 0.1
				text: "City: Nazareth Illit"
		BoxLayout:
			orientation: 'horizontal'
			Label:
				id: vol_rating
				color: 0, 0, 0, 1
				size_hint: .5, 0.1
				text: "Age: 17"
		BoxLayout:
			orientation: 'horizontal'
			Label:
				id: vol_rating
				color: 0, 0, 0, 1
				size_hint: .5, 0.1
				text: "Languages: Hebrew, English"
		


			
